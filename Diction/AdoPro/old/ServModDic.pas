{$R *.dfm}

procedure TDMServDic.NaturObjctImgQryNewRecord(DataSet: TDataSet);
begin
  DataSet.FieldByName('SHOWLOC').AsBoolean := True;
  DataSet.FieldByName('SHOWWEB').AsBoolean := True;
end;

procedure TDMServDic.TaxSystemDetailsQryCalcFields(DataSet: TDataSet);
begin
  if DataSet.FieldByName('PAYER').Value <> null then
  begin
    if DataSet.FieldByName('PAYER').Value = 1 then
      DataSet.FieldByName('CalcPayerTAX').AsString := DataSet.FieldByName('PDVSUM').AsString  +' %'
    else DataSet.FieldByName('CalcPayerTAX').AsString :='ͳ';
  end
  else DataSet.FieldByName('CalcPayerTAX').AsString :='';
end;

procedure TDMServDic.BrDriveQryNewRecord(DataSet: TDataSet);
begin
  DataSet.FieldByName('DRIVEKIND').Value := Elem_BrDrvKindQry.DB.fieldByName('DRIVEKIND').Value;
end;

procedure TDMServDic.BrCityQry_CalcFields(DataSet: TDataSet);
var CityKind: Integer;
begin
  CityKind := DataSet.FieldByName('CITYKIND').asInteger;
  if CityKind < 0 then DataSet.FieldByName('CalcCityType').asString := EmptyStr
  else DataSet.FieldByName('CalcCityType').asString := LoadResString( pCityIdents[CityKind] );
end;

end.
