  object PartnersQry: TIBQuery
    SQL.Strings = (
      'select C.Name,  C.CompanyId'
      'from companies C'
      
        'left join CntrEmbassies E on E.EmbassyId = C.CompanyId and E.Cou' +
        'ntryId = :CountryId'
      'where ((AsVisas = '#39'T'#39') or (AsAmbas = '#39'T'#39'))'
      '  and E.ParentId is null'
      'order by name')
    Left = 32
    Top = 144
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'CountryId'
        ParamType = ptUnknown
      end>
